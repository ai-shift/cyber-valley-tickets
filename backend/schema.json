                    "cancel_date",
                    "creator",
                    "days_amount",
                    "description",
                    "id",
                    "place",
                    "start_date",
                    "ticket_price",
                    "tickets_bought",
                    "tickets_required_until_cancel",
                    "title"
                ]
            },
            "EventPlace": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer",
                        "maximum": 9223372036854775807,
                        "minimum": -9223372036854775808,
                        "format": "int64"
                    },
                    "title": {
                        "type": "string",
                        "maxLength": 200
                    },
                    "max_tickets": {
                        "type": "integer",
                        "maximum": 9223372036854775807,
                        "minimum": 0,
                        "format": "int64"
                    },
                    "min_tickets": {
                        "type": "integer",
                        "maximum": 9223372036854775807,
                        "minimum": 0,
                        "format": "int64"
                    },
                    "min_price": {
                        "type": "integer",
                        "maximum": 9223372036854775807,
                        "minimum": 0,
                        "format": "int64"
