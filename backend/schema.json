                        "type": "integer",
                        "maximum": 9223372036854775807,
                        "minimum": 0,
                        "format": "int64"
                    },
                    "available": {
                        "type": "boolean"
                    },
                    "is_used": {
                        "type": "boolean",
                        "readOnly": true
                    }
                },
                "required": [
                    "days_before_cancel",
                    "id",
                    "is_used",
                    "max_tickets",
                    "min_days",
                    "min_price",
                    "min_tickets",
                    "title"
